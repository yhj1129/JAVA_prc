package ch20;

public class NestedLoopTest {

	public static void main(String[] args) {

		int dan = 2;
		int count = 1;
		
		for( ; dan < 10 ; dan++) {
			
			for( ; count < 10 ; count ++) {
				System.out.println(dan + " X " + count + " = " + dan*count);
			}
			
			System.out.println();
		}
	}

}
